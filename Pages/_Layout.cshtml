<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no"> <title>@ViewData["Title"] - kursinord.no</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/assets/css/site.css" />
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet">
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/assets/css/site.min.css" asp-append-version="true" />
        <link href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet">
    </environment>
    @RenderSection("HeadScripts", required: false)
</head>
<body>
<header>
<nav class="navbar navbar-expand-md navbar-dark bg-dark">
        <a asp-page="/Index" class="navbar-brand link-decoration-none">kursinord.no</a>

        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarsExampleDefault" aria-controls="navbarsExampleDefault" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarsExampleDefault">
                
        @await Html.PartialAsync("_LoginPartial")
        @await Html.PartialAsync("_NavAdminPartial")
    
        </div>
    </nav>
</header>
<main>
@RenderBody()
</main>
@await Html.PartialAsync("_PartFooter")

    <script src="~/assets/js/site.min.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
